{
    "name": "Chomusuke",
    "description": "Chomusuke will join you to fill your empty heart!",

    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,

    "entity_action_chosen": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:execute_command",
                "command": "summon cat ~ ~1 ~ {Silent:0b,Invulnerable:1b,variant:\"minecraft:all_black\",Tags:[\"chomusuke\"],CustomName:'{\"bold\":true,\"color\":\"dark_red\",\"text\":\"Chomusuke\"}'}"
            },
            {
                "type": "origins:execute_command",
                "command": "data modify entity @e[tag=chomusuke,limit=1,sort=nearest] Owner set from entity @s UUID"
            }
        ]
    }
}
