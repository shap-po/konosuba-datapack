{
    "name": "Steal",
    "description": "You can steal from people >:3",

    "badges": [
        {
            "type": "origins:keybind",
            "sprite": "origins:textures/gui/badge/active.png",
            "text": "Use %s with an empty hand while sneaking to try and steal from someone in front of you."
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/info.png",
            "text": "Make sure they can't see you!"
        }
    ],

    "type": "origins:active_self",
    "key": {
        "key": "key.origins.secondary_active"
    },

    "entity_action": {
        "type": "origins:raycast",

        "distance": 6,
        "bientity_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:target_condition",
                    "condition": { "type": "origins:entity_type", "entity_type": "minecraft:player" }
                },
                {
                    "type": "origins:actor_condition",
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            { "type": "origins:inventory", "slot": "weapon.mainhand", "comparison": "==", "compare_to": 0 },
                            { "type": "origins:sneaking" }
                        ]
                    }
                }
            ]
        },

