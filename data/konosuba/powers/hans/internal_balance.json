{
    "type": "origins:action_over_time",
    "interval": 100,

    "entity_action": {
        "type": "origins:if_else_list",
        "actions": [
            {
                "condition": { "type": "origins:resource", "resource": "*:hans/slime_count", "comparison": "<=", "compare_to": 1875 },
                "action": { "type": "origins:change_resource", "resource": "*:hans/slime_count", "change": 10 }
            },
            {
                "condition": { "type": "origins:resource", "resource": "*:hans/slime_count", "comparison": ">=", "compare_to": 8750 },
                "action": { "type": "origins:change_resource", "resource": "*:hans/slime_count", "change": -50 }
            },
            {
                "condition": { "type": "origins:resource", "resource": "*:hans/slime_count", "comparison": ">=", "compare_to": 6250 },
                "action": { "type": "origins:change_resource", "resource": "*:hans/slime_count", "change": -20 }
            }
        ]
    }
}
